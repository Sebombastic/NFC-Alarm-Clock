<?xml version="1.0" encoding="utf-8"?>
<navigation
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/nav_alarm_options"
	app:startDestination="@id/nacAlarmOptionsDialog">

	<dialog
		android:id="@+id/nacAlarmOptionsDialog"
		android:name="com.nfcalarmclock.alarm.options.NacAlarmOptionsDialog"
		android:label="NacAlarmOptionsDialog" >
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacDismissOptionsDialog"
			app:destination="@id/nacDismissOptionsDialog" />
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacSnoozeOptionsDialog"
			app:destination="@id/nacSnoozeOptionsDialog" />
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacAudioSourceDialog"
			app:destination="@id/nacAudioSourceDialog" />
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacTextToSpeechDialog"
			app:destination="@id/nacTextToSpeechDialog" />
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacUpcomingReminderDialog"
			app:destination="@id/nacUpcomingReminderDialog" />
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacFlashlightOptionsDialog"
			app:destination="@id/nacFlashlightOptionsDialog" />
		<action
			android:id="@+id/action_nacAlarmOptionsDialog_to_nacVolumeOptionsDialog"
			app:destination="@id/nacVolumeOptionsDialog" />
	</dialog>
	<dialog
		android:id="@+id/nacDismissOptionsDialog"
		android:name="com.nfcalarmclock.alarm.options.dismissoptions.NacDismissOptionsDialog"
		android:label="NacDismissOptionsDialog" />
	<dialog
		android:id="@+id/nacSnoozeOptionsDialog"
		android:name="com.nfcalarmclock.alarm.options.snoozeoptions.NacSnoozeOptionsDialog"
		android:label="NacSnoozeOptionsDialog" />
	<dialog
		android:id="@+id/nacAudioSourceDialog"
		android:name="com.nfcalarmclock.alarm.options.audiosource.NacAudioSourceDialog"
		android:label="NacAudioSourceDialog" />
	<dialog
		android:id="@+id/nacTextToSpeechDialog"
		android:name="com.nfcalarmclock.alarm.options.tts.NacTextToSpeechDialog"
		android:label="NacTextToSpeechDialog" />
	<dialog
		android:id="@+id/nacUpcomingReminderDialog"
		android:name="com.nfcalarmclock.alarm.options.upcomingreminder.NacUpcomingReminderDialog"
		android:label="NacUpcomingReminderDialog" />
	<dialog
		android:id="@+id/nacFlashlightOptionsDialog"
		android:name="com.nfcalarmclock.alarm.options.flashlight.NacFlashlightOptionsDialog"
		android:label="NacFlashlightOptionsDialog" />
	<dialog
		android:id="@+id/nacVolumeOptionsDialog"
		android:name="com.nfcalarmclock.alarm.options.volume.NacVolumeOptionsDialog"
		android:label="NacVolumeOptionsDialog" />
</navigation>